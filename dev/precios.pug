include modulos/cabecera.pug

<div class="animated fadeIn">
<div class="zona_texto">
    <h2>Precios</h2>
    <p>Calcule el precio según sus necesidades de personal</p>
</div>
<div class="zona_slider">
    <div class="caja_slider">
        <div class="selec_tra">
            <label for="num_traba">
                <input type="number" name="num_traba" value="1" min="1" max="100" />
                <p>Puede introducir el núm. de trabajadores</p>
            </label>
        </div>
        <input type="range" min="10" max="11" step="0.1" value="10.2" data-rangeslider="" style="position: absolute; width: 1px; height: 1px; overflow: hidden; opacity: 0;">
        <div class="rangeslider rangeslider--horizontal" id="js-rangeslider-2">
            <div class="rangeslider__fill" style="width: 124.6px;"></div>
