                <div class="rangeslider__num">5</div>
            </div>
        </div>
        <div class="resultado_slider">
            <ul>
                <li>
                    <span>3</span>empleados
                </li>
                <li>
                    <span>23</span>€ al mes
                </li>
            </ul>
        </div>
        <div class="selector_slider">
            <ul>
                <li>
                    <input type="radio" id="mes-option" name="selector" checked=checked>
                    <label for="mes-option">Mensual</label>
                    <div class="check">
                        <div class="inside"></div>
                    </div>
                </li>
                <li>
                    <input type="radio" id="ano-option" name="selector">
                    <label for="ano-option">Anual</label>
                    <div class="check">
                        <div class="inside"></div>
                    </div>
                </li>
            </ul>
        </div>
        <div class="zona_callto">
            <div class="callto">
                <ul>
